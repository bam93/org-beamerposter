\ProvidesPackage{beamerorgposter}
\selectcolormodel{<% color_model %>}
\mode<presentation>
% --------------------------------------------------------------------------------

% \usefonttheme{professionalfonts} prevents beamer from overwriting
% your fonts and is therefore needed to use specified font packages.
\usefonttheme{professionalfonts}

% use Adobe's Source Pro fonts:
% Source Serif Pro: https://github.com/adobe-fonts/source-serif-pro
% Source Sans Pro: https://github.com/adobe-fonts/source-sans-pro
% Source Code Pro: https://github.com/adobe-fonts/source-code-pro
% \setmainfont{<% fonts.serif %>}
\setsansfont{<% fonts.sans %>}
% \setmonofont{<% fonts.mono %>}
% \usepackage{fontspec}
% \usepackage{microtype}
% \usepackage{unicode-math}
\usepackage[lining]{<% fonts.package %>}
% \usepackage{sourceserifpro}
% \usepackage{sourcecodepro}
% \usepackage{FiraSans}
% \setmathfont{TeX Gyre Pagella Math}
