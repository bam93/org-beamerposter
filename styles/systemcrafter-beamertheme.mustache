% \urlstyle{same}


% --------------------------------------------------------------------------------
% custom colors
% --------------------------------------------------------------------------------

%% Donders Colors
<% #colors %>
\definecolor{<% name %>}{HTML}{<% hex %>}
<% /colors %>

% --------------------------------------------------------------------------------
% headline colors and fonts
% --------------------------------------------------------------------------------
\setbeamercolor{headline}{fg=<% headline.fg %>,bg=<% headline.bg %>}
\setbeamercolor{logo}{bg=<% logo.bg %>}
\setbeamercolor{title in headline}{fg=<% headline.title_fg %>,bg=<% headline.title_bg %>}
\setbeamercolor{author in headline}{fg=<% headline.author_fg %>,bg=<% headline.author_bg %>}
\setbeamercolor{institute in headline}{fg=<% headline.institute %>}
\setbeamercolor{logo in headline}{fg=<% headline.logo_fg %>,bg=<% headline.logo_bg %>}
\setbeamercolor{separation line}{bg=<% headline.sep_bg %>}

% --------------------------------------------------------------------------------
% footline colors and fonts
% --------------------------------------------------------------------------------
\setbeamercolor{footline}{fg=<% footline.fg %>,bg=<% footline.bg %>}
\setbeamerfont{footline}{size=\normalsize}
% \setbeamerfont{footline}{size=\small}

% --------------------------------------------------------------------------------
% body colors and fonts
% --------------------------------------------------------------------------------
\setbeamercolor*{normal text}{fg=<% body.fg %>,bg=<% body.bg %>}
\setbeamercolor{item}{fg=<% item.fg %>}

% --------------------------------------------------------------------------------
% block environment
% --------------------------------------------------------------------------------
\setbeamercolor*{block body}{bg=<% block.body_bg %>,fg=<% block.body_fg %>}
\setbeamerfont{block body}{size=\normalsize}
\setbeamercolor*{block title}{fg=<% block.title_fg %>,bg=<% block.title_bg %>}
\setbeamerfont{block title}{size=\Large,series=\bf}

% % --------------------------------------------------------------------------------
% % references environment
% % --------------------------------------------------------------------------------
% \newenvironment<>{references}[1]{%
% \setbeamerfont{block body}{size=\small}%
% \begin{block}#2{#1}}{\end{block}}

% \setbeamercolor{bibliography item}{parent=<% ref.author %>}
% \setbeamercolor{bibliography entry title}{use=normal text,fg=<% ref.title %>}
\setbeamercolor{bibliography entry author}{use=structure,fg=<% ref.author %>}
\setbeamercolor{bibliography entry location}{use=structure,fg=<% ref.location %>}
\setbeamercolor{bibliography entry note}{use=structure,fg=<% ref.note %>}

\hypersetup{
  colorlinks=true,
  linkcolor=<% ref.link %>,
  urlcolor=<% ref.url %>,
  citecolor=<% ref.cite %>
}

% Ne pas mettre en gras les mots comme "and others"
\DeclareDelimFormat[bib]{finalnamedelim}{\addspace\&\space}
\DeclareDelimFormat{multinamedelim}{\addcomma\space}
\DeclareDelimFormat{finalnamedelim}{\addcomma\space}

% Supprimer le gras de "et al."
\renewcommand*{\mkbibetal}[1]{#1}

% Ne pas mettre "in:" en gras
\renewcommand*{\inbibpunct}{\addcolon\space}
\renewcommand*{\bibsentencepunct}{\addperiod\space}
\DeclareFieldFormat[incollection]{booktitle}{#1}

% "pages" â†’ pas en gras
\DeclareFieldFormat{pages}{#1}
\DeclareFieldFormat{pagetotal}{#1}

% --------------------------------------------------------------------------------
% example environment
% --------------------------------------------------------------------------------
\setbeamercolor*{example title}{fg=<% example.title_fg %>,bg=<% example.title_bg %>}
\setbeamerfont{example title}{size=\large,series=\bf}

\setbeamerfont{alerted text}{series=\bf}
\setbeamercolor{alerted text}{fg=<% alerted_text.fg %>}

\setbeamertemplate{itemize items}[circle]
\setbeamertemplate{navigation symbols}{}  % no navigation on a poster

% --------------------------------------------------------------------------------
% caption
% --------------------------------------------------------------------------------
\setbeamerfont{caption name}{series=\bfseries,size={\small}}
\setbeamerfont{caption}{size={\small}}
\setbeamercolor{caption name}{fg=<% caption.name_fg %>}

% --------------------------------------------------------------------------------
% block
% --------------------------------------------------------------------------------
\setbeamertemplate{block begin}{
  \vskip.25ex
  \IfStrEq{\insertblocktitle}{}{}{
    \vskip.5ex
  \begin{beamercolorbox}[leftskip=1cm,colsep*=.75ex]{block title}%
    \usebeamerfont*{block title}\Large\insertblocktitle
  \end{beamercolorbox}%
  }
  {\ifbeamercolorempty[bg]{block body}{}{\nointerlineskip\vskip-0.5pt}}%
  \usebeamerfont{block body}%
  \begin{beamercolorbox}[colsep*=.75ex,sep=.75ex,vmode]{block body}%
    \ifbeamercolorempty[bg]{block body}{\vskip-.25ex}{\vskip-.75ex}\vbox{}%
  }
  \setbeamertemplate{block end}{
  \end{beamercolorbox}
}
% --------------------------------------------------------------------------------

