      % \vskip1.5cm
      % \centering
      \vskip7.6ex
      \usebeamercolor{title in
        headline}{\color{fg}\setlength\lineskip{10pt}\textbf{\Huge{\inserttitle}}\\[1ex]}
      \vskip2.6ex
      \usebeamercolor{author in headline}{\color{fg}\large{\insertauthor}\\[1ex]}
      \vskip2.4ex
    \end{beamercolorbox}
    \column{.3\textwidth}
    \begin{beamercolorbox}[wd=\columnwidth]{logo}
      % \centering
      % \vskip2.10cm
